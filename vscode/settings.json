    "todohighlight.isEnable": true,
    "todohighlight.keywords": [
        {
            "text": "TODO",
            "regex": {
                "pattern": "(TODO|CMBK)(\\s?[\\(\\[]\\S+?[\\)\\]])?:?|[\\\\@]todo"
            },
            "color": "#fafbfc",
            "backgroundColor": "#fb8532",
            "overviewRulerColor": "#fb8532"
        },
        {
            "text": "DEPRECATE",
            "regex": {
                "pattern": "DEPRECATED(\\s?[\\(\\[]\\S+?[\\)\\]])?:?|[\\\\@]deprecated"
            },
            "color": "#fafbfc",
            "backgroundColor": "#f00",
            "overviewRulerColor": "#f00"
        },
        {
            "text": "DEBUG",
            "regex": {
                "pattern": "DEBUG(\\s?[\\(\\[]\\S+?[\\)\\]])?:?|[\\\\@]bug"
            },
            "color": "#fafbfc",
            "backgroundColor": "#ea4a5a",
            "overviewRulerColor": "#ea4a5a"
        },
        {
            "text": "NOTE",
            "regex": {
                "pattern": "NOTE(\\s?[\\(\\[]\\S+?[\\)\\]])?:?|[\\\\@]note"
            },
            "color": "#c8e1ff",
            "backgroundColor": "#0000",
            "overviewRulerColor": "#0000"
        },
        {
            "text": "HACK",
            "regex": {
                "pattern": "HACK(\\s?[\\(\\[]\\S+?[\\)\\]])?:?",
            },
            "color": "#f9b3dd",
            "backgroundColor": "#0000",
            "overviewRulerColor": "#0000"
        },
        {
            "text": "PERF",
            "regex": {
                "pattern": "PERF(\\s?[\\(\\[]\\S+?[\\)\\]])?:?",
            },
            "color": "#fafbfc",
            "backgroundColor": "#28a745",
            "overviewRulerColor": "#28a745"
        },
    ],

