    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [ "<bs>" ],
            "commands": [
                ":nohl",
                "workbench.action.closePanel",
                "notifications.clearAll"
            ]
        },
        {
            "before": [ "<leader>", "s", "c" ],
            "commands": [ ":nohl" ]
        },
        {
            "before": [ "<C-e>" ],
            "after": [ "3", "<C-e>" ]
        },
        {
            "before": [ "<C-y>" ],
            "after": [ "3", "<C-y>" ]
        },
        {
            "before": [ "<C-j>" ],
            "commands": [ "workbench.action.togglePanel" ]
        },
        {
            "before": [ "z", "j" ],
            "commands": [ "editor.gotoNextFold" ]
        },
        {
            "before": [ "z", "k" ],
            "commands": [ "editor.gotoPreviousFold" ]
        },
        {
            "before": [ "<leader>", "<leader>", "<leader>" ],
            "commands": [ "workbench.action.showCommands" ]
        },
        {
            "before": [ "<leader>", "l" ],
            "after": [ "g", "t" ]
        },
        {
            "before": [ "<leader>", "h" ],
            "after": [ "g", "T" ]
        },
        {
            "before": [ "<leader>", "f", "s" ],
            "commands": [ "workbench.action.files.save" ]
        },
        {
            "before": [ "<leader>", "f", "f" ],
            "commands": [ "workbench.action.quickOpen" ]
        },
        {
            "before": [ "<leader>", "f", "t" ],
            "commands": [ "workbench.action.toggleSidebarVisibility" ]
        },
        {
            "before": [ "<leader>", "t", "t" ],
            "commands": [ "workbench.action.toggleTabsVisibility" ]
        },
        {
            "before": [ "<leader>", "b", "b" ],
            // "commands": [ "workbench.action.showEditorsInActiveGroup" ]
            "commands": [ "workbench.action.showEditorsInGroup" ]
        },
        {
            "before": [ "<leader>", "b", "k" ],
            "commands": [ "workbench.action.keepEditor" ]
        },
        {
            "before": [ "<leader>", "b", "d" ],
            "commands": [ "workbench.action.closeActiveEditor" ]
        },
        {
            "before": [ "<leader>", "b", "D" ],
            "commands": [ "workbench.action.closeOtherEditors" ]
        },
        {
            "before": [ "<leader>", ">", ">" ],
            "commands": [ "workbench.action.moveEditorRightInGroup" ]
        },
        {
            "before": [ "<leader>", "<", "<" ],
            "commands": [ "workbench.action.moveEditorLeftInGroup" ]
        },
        {
            "before": [ "<leader>", "H" ],
            "commands": [ "workbench.action.moveEditorToLeftGroup" ]
        },
        {
            "before": [ "<leader>", "J" ],
            "commands": [ "workbench.action.moveEditorToBelowGroup" ]
        },
        {
            "before": [ "<leader>", "K" ],
            "commands": [ "workbench.action.moveEditorToAboveGroup" ]
        },
        {
            "before": [ "<leader>", "L" ],
            "commands": [ "workbench.action.moveEditorToRightGroup" ]
        },
        {
            "before": [ "<leader>", "w", "d" ],
            "commands": [ "workbench.action.closeEditorsInGroup" ]
        },
        {
            "before": [  "<leader>", "w", "V" ],
            "commands": [ "workbench.action.splitEditorRight" ]
        },
        {
            "before": [  "<leader>", "w", "S" ],
            "commands": [ "workbench.action.splitEditorDown" ]
        },
        {
            "before": [ "<leader>", "w", "h" ],
            "commands": [ "workbench.action.focusLeftGroup" ]
        },
        {
            "before": [ "<leader>", "w", "j" ],
            "commands": [ "workbench.action.focusBelowGroup" ]
        },
        {
            "before": [ "<leader>", "w", "k" ],
            "commands": [ "workbench.action.focusAboveGroup" ]
        },
        {
            "before": [ "<leader>", "w", "l" ],
            "commands": [ "workbench.action.focusRightGroup" ]
        },
        {
            "before": [ "<leader>", "w", "H" ],
            "commands": [ "workbench.action.moveActiveEditorGroupLeft" ]
        },
        {
            "before": [ "<leader>", "w", "J" ],
            "commands": [ "workbench.action.moveActiveEditorGroupDown" ]
        },
        {
            "before": [ "<leader>", "w", "K" ],
            "commands": [ "workbench.action.moveActiveEditorGroupUp" ]
        },
        {
            "before": [ "<leader>", "w", "L" ],
            "commands": [ "workbench.action.moveActiveEditorGroupRight" ]
        },
        {
            "before": [ "<leader>", "a", "p" ],
            "commands": [ "editor.action.peekDefinition" ]
        },
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [ ">" ],
